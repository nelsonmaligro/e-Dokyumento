      <div class="header-menu">

        <div class="col-sm-12">
          <a id="menuToggle" class="menutoggle pull-left"><i class="fa fa fa-tasks"></i></a>
          <div class="header-left">

            <!-- notification-->
            <div id="notifyme" class="dropdown for-notification float-left">
              <button class="btn btn-secondary dropdown-toggle" type="button" id="notification" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <i class="fa fa-bell"></i>
                <span id=notiNr class="count bg-danger"><%= files.length %></span>
              </button>
              <div class="dropdown-menu " aria-labelledby="notification">
                <li> <a id='notiLabel' style="font-size:13px;">&nbsp;&nbsp; You have  <%= files.length %> incoming files pending</a></li>
                <div id='addHere'>
                  <% for (var i=0; i < files.length; i++){ %>
                    <li>
                      <a style="background-color:Red;color:white;" class="dropdown-item media" href="/incoming/<%= files[i] %>">
                        <i class="fa fa-check"></i>
                        <p style='color:white;'><%= files[i] %></p>
                      </a>
                    </li>
                    <% } %>
                  </div>
                </div>
              </div>
              <div id="releaseme" class="dropdown for-notification float-left">
                <button class="btn btn-secondary dropdown-toggle" type="button" id="releasenoti" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  <i class="fa fa-star-o"></i>
                  <span id=releaseNr class="count bg-danger" ><%= release.length %></span>
                </button>
                <div class="dropdown-menu " aria-labelledby="notification">
                  <li> <a id='releaseLabel' style="font-size:13px;">&nbsp;&nbsp; You have  <%= release.length %> For-Release files pending</a></li>
                  <div id='addRelease'>
                    <% for (var i=0; i < release.length; i++){ %>
                      <li>
                        <a class="dropdown-item media bg-flat-color-3" href="/incoming/release/<%= release[i] %>">
                          <i class="fa fa-check"></i>
                          <p><%= release[i] %></p>
                        </a>
                      </li>
                      <% } %>
                    </div>
                  </div>
                </div>

              <form name="formroute" id="formroute">
                <!-- form send to branch-->
                <div class="col col-sm-4 float-left"><input value= "<%= disp %>" type="text" id="newfile" name="newfile" class="input-sm form-control-sm form-control"></div>
                <div class=" float-left"><input value= "<%= disp %>" type="hidden" id="fileroute"  name="fileroute"></div>

                <!-- if AI completes-->
                <div id="actBr" class="float-left" style="display: none;">
                  <span class="badge badge-info">Action Branch</span>
                </div>

                <button style="display:none;" id= "butScan" class="col col-sm-3 btn btn-primary float-left btn-sm" type="button" disabled>
                  <span class="spinner-border spinner-border-sm " role="status" aria-hidden="true"></span>
                  <span clas="col-sm-0 " id="txtscan" >&nbsp;Scanning document using OCR...</span>
                </button>


                <div class="col col-sm-2 float-left" id='selDiv' style="display: none;">
                  <select id="selBr" data-placeholder="Select Branch" multiple class="standardSelect" tabindex="5" name="selBr">
