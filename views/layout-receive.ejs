                              <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                              <button id='butConfirm' type="button" class="btn btn-primary">Confirm</button>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <!-- Folder Browse Modal-->

              <!-- sidebar -->
              <div id="sideToggle" class="hamburger">
