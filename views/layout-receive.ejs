                      <li><a class="nav-link" href="/myprofile"><i class="fa fa-user"></i> My Profile</a></li>
                      <li><a class="nav-link" onclick="popUpQR()" href="#"><i class="fa fa-qrcode"></i> My QR Code</a></li>

                      <li class="dropdown-submenu">
                        <a class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" href="#"><i class="fa fa-envelope"></i> Mail <span class="count"><%= mailfiles.length %></span></a>
                        <ul id='mailnoti'class="dropdown-menu" style="left:-220px!important;width:210px!important;padding-left:5px;">
                          <% mailfiles.forEach (function (file){
                            var fileArr = file.split('/'); filename = fileArr[fileArr.length - 1].substring(0,20); %>
                            <li><button type='button' onclick="delNotiFile('<%= file %>')" class='btn btn-danger btn-sm fa fa-times' href="#"></button><button button type='button' class="btn btn-link btn-sm" onclick="openDisFile('<%= file %>')" href="#"><%= filename %></button></li>
                            <% }); %>
                          </ul>
                        </li>
                        <!-- enable this when need arises-->
                        <!--
                        <% if ((level.toUpperCase()=='SECRETARY') || (level.toUpperCase()=='STAFF')) { %>
                        <li><a id="switchDuty" class="nav-link"  href="#"><i class="fa fa-random"></i> Toggle Duty</a></li>
                        <% } %>
                        -->
                        <li><a id="disCheckDrive" class="nav-link"  href="#"><i class="fa fa-cog"></i> Check map drive</a></li>
                        <li><a id="disLogout" class="nav-link" href="#"><i class="fa fa-power-off"></i> Logout</a></li>
                      </div>
                    </div>

                  </div>
                </div>

